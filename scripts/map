sed 's/IF/k_if/g' |
sed 's/INT/k_int/g' |
sed 's/LONG/k_long/g' |
sed 's/REGISTER/k_register/g' |
sed 's/RETURN/k_return/g' |
sed 's/SHORT/k_short/g' |
